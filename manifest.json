{
	"manifest_version": 2,

	"name": "Kill9gagFeed",
	"description": "This extension removes the 9gag feed. Enable this while at work.",

	"version": "0.1",

	"permissions": [
		"http://9gag.com/"
	],

	"content_scripts": [
		{
			"matches": ["http://9gag.com/"],
			"js": ["pages/script.js"]	
		}
	],

	"icons": {
		"16": "icons/icon-16.png",
		"32": "icons/icon-32.png",
		"64": "icons/icon-64.png"
	}
}